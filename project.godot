config_version=5

[application]

config/name="Not Pokemon Again"
run/main_scene="res://scenes/test_world_scene.tscn"
config/features=PackedStringArray("4.2", "Mobile")
config/icon="res://icon.svg"

[autoload]

AllCreatures="*res://scripts/all_creatures.gd"
PlayerData="*res://scripts/player_data.gd"
BattleSetupHandler="*res://scripts/battle_setup_handler.gd"
BattleSceneController="*res://scripts/battle_scene_controller.gd"
Testing="*res://scripts/testing.gd"
SaveHandler="*res://scripts/save_handler.gd"
WorldScenes="*res://scripts/world_scenes.gd"
SceneLoader="*res://scripts/scene_loader.gd"
Groups="*res://resources/enums/groups.gd"
Dialogic="*res://addons/dialogic/Other/DialogicGameHandler.gd"

[debug]

gdscript/warnings/untyped_declaration=1
gdscript/warnings/unsafe_property_access=1
gdscript/warnings/unsafe_method_access=1
gdscript/warnings/unsafe_cast=1
gdscript/warnings/unsafe_call_argument=1

[display]

window/size/initial_position_type=0
window/stretch/mode="canvas_items"

[dotnet]

project/assembly_name="Not Pokemon Again"

[editor]

export/convert_text_resources_to_binary=false

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg")

